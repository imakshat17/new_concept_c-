#include<iostream>
using namespace std;
int multiply(int x, int y){
     return x*y;
}
int add(int x,int y){
     return x+y;
}
void perform(int(*operation)(int,int),int x,int y){
    int result=operation(x,y);
    cout<<result<<endl;
    
}
int main(){
    int(*operation)(int,int)=multiply;
    perform(operation,2,4);
       return 0;
}
